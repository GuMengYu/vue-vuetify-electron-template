{
  "name": "vue-electron-vuetify",
  "version": "0.0.2",
  "private": true,
  "main": "dist-electron/main/index.js",
  "author": "gumengyu",
  "description": "vue + electron + vuetify template",
  "scripts": {
    "preview": "vite preview",
    "dev": "vite",
    "build": "vite build",
    "electron:dev": "vite --mode electron",
    "electron:build": "npm run electron:prebuild && electron-builder",
    "electron:build:mac": "npm run electron:prebuild && electron-builder --mac",
    "electron:build:win": "npm run electron:prebuild && electron-builder --win",
    "electron:build:linux": "npm run electron:prebuild && electron-builder --linux",
    "electron:prebuild": "vite build --mode electron",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:ts",
    "lint:eslint": "eslint \"src/**/*.{js,jsx,ts,tsx,vue}\" --fix",
    "lint:prettier": "prettier --write \"src/**/*.{js,tsx,css,scss,vue,md,json,html}\"",
    "lint:ts": "tsc --noEmit --skipLibCheck",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "dev:https": "vite build && npm run serve",
    "serve": "serve dist"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-log": "^4.4.7",
    "express": "^4.17.3",
    "polka": "^0.5.2",
    "sirv": "^2.0.2",
    "electron-store": "^8.1.0",
    "http-proxy-middleware": "^2.0.6",
    "vue": "^3.2.41",
    "vue-content-loader": "^2.0.1",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.14",
    "vue-toastification": "^2.0.0-rc.5",
    "vuetify": "^3.2.4",
    "webfontloader": "^1.6.28",
    "vuetify-ctx-menu": "git+https://github.com/GuMengYu/vue3-context-menu.git#v0.0.2",
    "lottie-web": "^5.9.2",
    "overlayscrollbars": "^1.13.1",
    "pinia": "^2.0.13",
    "dayjs": "^1.11.1",
    "electron": "^24.1.1"
  },
  "devDependencies": {
    "@material/material-color-utilities": "^0.2.0",
    "@mdi/js": "^7.2.96",
    "@types/howler": "^2.2.7",
    "@types/lodash-es": "^4.17.6",
    "@types/overlayscrollbars": "^1.12.1",
    "@types/polka": "^0.5.4",
    "@types/webfontloader": "^1.6.34",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vueuse/components": "9.8.1",
    "@vueuse/core": "9.8.1",
    "@vueuse/electron": "9.8.1",
    "electron-builder": "^23.0.3",
    "electron-dl": "^3.3.1",
    "electron-is": "^3.0.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vue": "^8.7.0",
    "https-localhost": "^4.7.1",
    "husky": "^7.0.0",
    "lint-staged": "^12.4.1",
    "lodash-es": "^4.17.21",
    "prettier": "^2.6.2",
    "sass": "^1.38.0",
    "typescript": "^4.6.3",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.0.3",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.11.3",
    "vite-plugin-pwa": "^0.14.1",
    "vite-plugin-resolve": "^2.3.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vue-tsc": "^1.0.8"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,less,styl,html}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
